    <unicorn>
      <configurations>
        <configuration name="Feature.MyPosts" description="Feature MyPosts" dependencies="Foundation.*" extends="Helix.Feature">
          <predicate>
            <include name="MyPosts.Content" database="master" path="/sitecore/content/global/datasources" />
          </predicate>
        </configuration>
      </configurations>
    </unicorn>
  </sitecore>
</configuration>
